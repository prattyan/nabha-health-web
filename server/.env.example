# Server
PORT=4000
NODE_ENV=development

# Allowed frontend origin(s) for CORS
CORS_ORIGINS=http://localhost:5173

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nabha_health?schema=public

# Auth
JWT_ACCESS_SECRET=change-me-access-secret-please
JWT_REFRESH_SECRET=change-me-refresh-secret-please
JWT_ACCESS_TTL_SECONDS=900
JWT_REFRESH_TTL_SECONDS=2592000

# Field encryption (AES-256-GCM)
ENCRYPTION_KEY_BASE64=change-me-32-bytes-base64
